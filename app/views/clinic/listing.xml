<Alloy>
	<Window navTintColor="#CE1D1C" id="win" title="Clinic Type List"  navBarHidden="false">
		<ActionBar platform="android" displayHomeAsUp="true" onHomeIconItemSelected="closeWindow" />
		<RightNavButton platform="ios">
			<ImageView right='10'  onClick="doRefresh" width="25" height="25" image="/images/icon_refresh.png" />  
		</RightNavButton> 
		<View layout="vertical" id="loadingBar" height="120" width="120" borderRadius="15"  backgroundColor="#2E2E2E" >
		    <ActivityIndicator  id="activityIndicator" />
			<Label top="5" text="Loading" color="#ffffff" />
		</View>
		<View class="wfill hsize vert" top="0">
			<Label class="h5 wfill hsize themeColor padding" id="label_ida">Available Healthcare</Label>
			<TableView id="tblview" width="Ti.UI.FILL" height="Ti.UI.FILL" top="0"></TableView>
	 	</View> 
	    
    </Window>
</Alloy>   